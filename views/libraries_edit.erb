<h1>Edit: <%= @library.branch_name %></h1>

<form action="/libraries" method="post">
  Branch Name:<input type="text" name="branch_name", value="<%= @library.branch_name %>"></br>
  Phone Number:<input type="text", name="phone_number", value="<%= @library.phone_number %>"></br>
  Address:<input type="text", name="address", value="<%= @library.address %>"></br>

  <input type="submit" value="Update Library!">
</form>